== Principal ==
  # Fundo do site
    THIS2: body_background
    WEBY:
      html{ background: VAR; }

  # Borda dos menus
    THIS2: body_background
    WEBY:
      aside menu li,
      header nav menu li,
      footer nav menu li { border:1px solid VAR; }

  # Fundo Tabela frontal das páginas
    THIS2: cor_borda_noticias
    WEBY:
