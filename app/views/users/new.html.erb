<fieldset>
  <legend><%= t"new_param", :param => (t"user", :count => 1) %></legend>
  <%= form_for @user, :builder => SemanticFormBuilder, :url => users_path do |f| %>
    <% @user.errors.full_messages.each do |msg| %>  
      <p> <%= msg %> <p>
    <% end %>  
    <%= render :partial => "form", :object => f %>
    <p> <%= f.submit t"create" %> </p>
  <% end %>
</fieldset>
