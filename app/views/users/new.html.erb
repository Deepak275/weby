<fieldset>
  <legend><%= t:new_param, :param => t('user') %></legend>
<%= form_for @user, :url => users_path do |f| %>
  <% @user.errors.full_messages.each do |msg| %>  
    <%= msg %><br />
  <% end %>  
  <%= render :partial => "form", :object => f %>
  <%= f.submit t:send %>
<% end %>
</fieldset>
