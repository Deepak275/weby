<meta property=”og:title” content=”<%= @site.try(:title) || 'Weby' %><%= yield :title %>” />
<%= yield :meta_images %>
<% if current_site.repository %>
  <meta property=”og:image” content=”<%= asset_url(current_site.repository.archive.url(:m)) %>” />
<% end %>
<meta property=”og:site_name” content=”<%= current_site.title %>” />
