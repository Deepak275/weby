<fieldset>
  <legend><%= t 'listing_param', :param => t('feedback', :count => 2) %></legend>
  <table class='list'>
    <thead>
      <th><%= t 'name' %></th>
      <th><%= t 'email' %></th>
      <th><%= t 'subject' %></th>
      <th><%= t 'message' %></th>
      <th></th>
    </thead>
    <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr class="<%= cycle 'odd', 'even' %>">
        <td><%= feedback.name %></td>
        <td><%= feedback.email %></td>
        <td><%= feedback.subject %></td>
        <td><%= feedback.message %></td>
        <td>
          <%= raw make_menu(feedback) %> 
        </td>
      </tr>
    <% end; reset_cycle %>
    </tbody>
  </table>
</fieldset>
